{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../out-tsc/app",
		"paths": {
			"@ansyn/*": ["app/@ansyn/*"],
			"@builder/*": ["app/builder/*"],
			"@ansyn/submodules/*": ["app/submodules/*"]
		},
		"baseUrl": "./",
		"module": "es2015",
		"target": "es5",
		"types": []
	},
	"exclude": [
		"app/submodules/submodules/*.ts",
		"test.ts",
		"**/*.spec.ts"
	]
}
