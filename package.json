{
	"name": "@ansyn/ansyn",
	"version": "0.2.2",
	"license": "MIT",
	"scripts": {
		"postinstall": "npm run links",
		"links": "npm link ./src/assets ./src/app/app ./src/app/packages/router ./src/app/packages/login ./src/app/packages/open-layer-visualizers ./src/app/packages/context ./src/app/packages/overlays ./src/app/packages/core ./src/app/packages/menu ./src/app/packages/menu-items ./src/app/packages/imagery ./src/app/packages/open-layers-map ./src/app/packages/map-facade ./src/app/packages/cesium-map ./src/app/packages/status-bar ./src/app/packages/open-layer-center-marker-plugin",
		"unlinks": "npm unlink @ansyn/assets @ansyn/app @ansyn/router @ansyn/login @ansyn/context @ansyn/core @ansyn/menu @ansyn/menu-items @ansyn/imagery @ansyn/overlays @ansyn/open-layers-map @ansyn/map-facade @ansyn/cesium-map @ansyn/status-bar @ansyn/open-layer-center-marker-plugin @ansyn/open-layer-visualizers",
		"start": "ng serve",
		"build": "ng build",
		"start:aot": "ng serve --aot true",
		"build:aot": "ng build --aot true",
		"build:prod": "ng build --prod --sourcemap",
		"start:backwards": "ng serve --app backwards",
		"build:backwards": "ng build --app backwards",
		"build:backwards:prod": "ng build --app backwards --prod --sourcemap --build-optimizer=false",
		"test": "ng test",
		"test:single-run": "ng test --single-run",
		"lint": "ng lint --type-check",
		"log": "git log master --pretty=format:\\\"%an\\\",\\\"%ai\\\",\\\"%s\\\",\\\"%H\\\"' > log.csv && echo Commiter, Date, Message, Hash | cat - log.csv > log2.csv && mv log2.csv log.csv",
		"e2e": "ng e2e",
		"fix-packages": "node scripts/fix-packages",
		"fix-package-json": "node scripts/fix-package-json",
		"fix-all": "npm run fix-packages && npm run fix-package-json",
		"check-template": "node scripts/check-tmpl",
		"docker:elastic": "docker run -p 9200:9200 -v  /esdata1:/usr/share/elasticsearch/data -e \"http.host=0.0.0.0\" -e \"transport.host=127.0.0.1\" -e \"http.cors.enabled=true\" -e \"http.cors.allow-origin=/.*/\" -e \"http.cors.allow-credentials=true\" -e \"http.cors.allow-headers=X-Requested-With, Content-Type, Content-Length, Authorization\" docker.elastic.co/elasticsearch/elasticsearch:5.4.3"
	},
	"precommit": [
		"fix-all",
		"lint",
		"check-template"
	],
	"private": true,
	"dependencies": {
		"@angular/animations": "5.1.0",
		"@angular/common": "5.1.0",
		"@angular/compiler": "5.1.0",
		"@angular/core": "5.1.0",
		"@angular/forms": "5.1.0",
		"@angular/http": "5.1.0",
		"@angular/platform-browser": "5.1.0",
		"@angular/platform-browser-dynamic": "5.1.0",
		"@angular/router": "5.1.0",
		"@ngrx/core": "1.2.0",
		"@ngrx/effects": "4.1.1",
		"@ngrx/store": "4.1.1",
		"@turf/turf": "4.7.3",
		"angular-tree-component": "6.1.0",
		"angular2-uuid": "1.1.1",
		"ansyn-event-drops": "0.4.12",
		"core-js": "2.5.2",
		"crypto-js": "3.1.9-1",
		"d3": "4.11.0",
		"debounce": "1.0.0",
		"enhanced-resolve": "3.3.0",
		"flatpickr": "4.1.2",
		"font-awesome": "4.7.0",
		"lodash": "4.17.4",
		"lodash-decorators": "^4.5.0",
		"ng-infinitescroll": "1.0.12",
		"ol": "4.6.3",
		"primeng": "4.1.3",
		"proj4": "2.4.3",
		"reselect": "3.0.0",
		"rison": "0.1.1",
		"rxjs": "5.5.2",
		"wellknown": "0.5.0",
		"zone.js": "0.8.14"
	},
	"devDependencies": {
		"@angular/cli": "1.5.5",
		"@angular/compiler-cli": "5.1.0",
		"@angular/language-service": "5.1.0",
		"@types/d3": "4.11.1",
		"@types/geojson": "1.0.6",
		"@types/jasmine": "2.5.53",
		"@types/jasminewd2": "2.0.2",
		"@types/lodash": "4.14.88",
		"@types/node": "6.0.60",
		"@types/rison": "0.0.6",
		"@types/wellknown": "0.5.0",
		"codelyzer": "4.0.1",
		"jasmine": "latest",
		"jasmine-core": "2.6.2",
