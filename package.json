{
	"name": "@ansyn/ansyn",
	"version": "0.2.81",
	"license": "MIT",
	"scripts": {
		"start": "node scripts/generate-config.js && ng serve --sourcemaps --extract-css",
		"build": "ng build",
		"start:aot": "ng serve --aot true",
		"build:aot": "ng build --aot true",
		"build:prod": "node --max_old_space_size=12288 $(npm bin)/ng build --prod --build-optimizer --sourcemap",
		"build:builder": "ng build --app builder",
		"build:gulp": "cd build && gulp build",
		"test": "ng test",
		"test:single-run": "ng test --single-run",
		"test:e2e": "node scripts/generate-config.js && ng e2e",
		"lint": "ng lint --type-check",
		"log": "git log master --pretty=format:\\\"%an\\\",\\\"%ai\\\",\\\"%s\\\",\\\"%H\\\" > log.csv && echo Commiter, Date, Message, Hash | cat - log.csv > log2.csv && mv log2.csv log.csv",
		"ng": "ng",
		"fix-packages": "node scripts/fix-packages",
		"fix-package-json": "node scripts/fix-package-json",
		"fix-all": "node scripts/fix-package-json -a",
		"generate-config": "node scripts/generate-config.js",
		"docker:elastic": "docker run -p 9200:9200 -v  /esdata1:/usr/share/elasticsearch/data -e \"http.host=0.0.0.0\" -e \"transport.host=127.0.0.1\" -e \"http.cors.enabled=true\" -e \"http.cors.allow-origin=/.*/\" -e \"http.cors.allow-credentials=true\" -e \"http.cors.allow-headers=X-Requested-With, Content-Type, Content-Length, Authorization\" docker.elastic.co/elasticsearch/elasticsearch:5.4.3"
	},
	"precommit": [
		"fix-all",
		"lint"
	],
	"private": true,
	"dependencies": {
		"@angular/animations": "5.1.2",
