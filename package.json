{
	"name": "@ansyn/ansyn",
	"version": "0.2.0",
	"license": "MIT",
	"scripts": {
		"postinstall": "npm run links",
		"links": "npm link ./src/assets ./src/app/app ./src/app/packages/router ./src/app/packages/login ./src/app/packages/open-layer-visualizers ./src/app/packages/context ./src/app/packages/overlays ./src/app/packages/core ./src/app/packages/menu ./src/app/packages/menu-items ./src/app/packages/imagery ./src/app/packages/open-layers-map ./src/app/packages/map-facade ./src/app/packages/cesium-map ./src/app/packages/status-bar ./src/app/packages/open-layer-center-marker-plugin",
		"unlinks": "npm unlink @ansyn/assets @ansyn/app @ansyn/router @ansyn/login @ansyn/context @ansyn/core @ansyn/menu @ansyn/menu-items @ansyn/imagery @ansyn/overlays @ansyn/open-layers-map @ansyn/map-facade @ansyn/cesium-map @ansyn/status-bar @ansyn/open-layer-center-marker-plugin @ansyn/open-layer-visualizers",
		"start": "ng serve",
		"build": "ng build",
		"start:aot": "ng serve --aot true",
		"build:aot": "ng build --aot true",
		"build:prod": "ng build --prod --sourcemap",
		"start:backwards": "ng serve --app backwards",
		"build:backwards": "ng build --app backwards --prod --sourcemap",
		"test": "ng test",
		"test:single-run": "ng test --single-run",
		"lint": "ng lint --type-check",
		"log": "git log --pretty=format:\"%H | %<(15)%an | %ad | %s%n\" --since=\"12 hours ago\" --date=\"iso8601\" > git-hash-log.txt",
		"e2e": "ng e2e",
		"fix-packages": "node scripts/fix-packages",
		"check-template": "node scripts/check-tmpl",
		"docker:elastic": "docker run -p 9200:9200 -v  /esdata1:/usr/share/elasticsearch/data -e \"http.host=0.0.0.0\" -e \"transport.host=127.0.0.1\" -e \"http.cors.enabled=true\" -e \"http.cors.allow-origin=/.*/\" -e \"http.cors.allow-credentials=true\" -e \"http.cors.allow-headers=X-Requested-With, Content-Type, Content-Length, Authorization\" docker.elastic.co/elasticsearch/elasticsearch:5.4.3"
	},
	"precommit": [
		"lint",
		"fix-packages",
		"check-template"
	],
	"private": true,
