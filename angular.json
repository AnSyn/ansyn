{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ansyn": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "showCircularDependencies": true,
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/@ansyn"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "dev": {
              "sourceMap": true,
              "extractCss": true,
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ansyn:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "ansyn:build:dev"
            },
            "production": {
              "browserTarget": "ansyn:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ansyn:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/@ansyn"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**/*"
            ]
          }
        }
      }
    },
    "ansyn-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ansyn:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**/*"
            ]
          }
        }
      }
    },
    "builder": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "build/buildBundle/dist",
            "index": "src/index.html",
            "main": "src/app/builder/main.builder.ts",
            "tsConfig": "src/tsconfig.builder.json",
            "showCircularDependencies": true,
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/@ansyn"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "builder:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "builder:build:dev"
            },
            "production": {
              "browserTarget": "builder:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "builder:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**/*"
            ]
          }
        }
      }
    },
    "builder-dev": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "build/buildBundle/dist",
            "index": "src/app/builder/dev/index.html",
            "main": "src/app/builder/dev/devApp.main.ts",
            "tsConfig": "src/tsconfig.builder.json",
            "showCircularDependencies": true,
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/@ansyn"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/configuration/configuration.ts",
                  "with": "src/configuration/configuration.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "builder-dev:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "builder-dev:build:dev"
            },
            "production": {
              "browserTarget": "builder-dev:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "builder-dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/undefined",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/app/@ansyn/assets/styles/styles.less"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/@ansyn/assets",
                "output": "/assets"
              },
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**/*"
            ]
          }
        }
      }
    },
    "@ansyn/core": {
      "root": "projects/ansyn/core",
      "sourceRoot": "src/app/@ansyn/core",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ansyn/core/tsconfig.lib.json",
            "project": "projects/ansyn/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ansyn/core/ng-package.json"
            }
          }
        }
      }
    },
    "@ansyn/imagery": {
      "root": "projects/ansyn/imagery",
      "sourceRoot": "src/app/@ansyn/imagery",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ansyn/imagery/tsconfig.lib.json",
            "project": "projects/ansyn/imagery/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ansyn/imagery/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ansyn/imagery/src/test.ts",
            "tsConfig": "projects/ansyn/imagery/tsconfig.spec.json",
            "karmaConfig": "projects/ansyn/imagery/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ansyn/imagery/tsconfig.lib.json",
              "projects/ansyn/imagery/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ansyn",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ansyn",
      "styleext": "less"
    },
    "@schematics/angular:directive": {
      "prefix": "ansyn"
    }
  }
}
