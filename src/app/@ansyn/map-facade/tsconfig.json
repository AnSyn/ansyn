{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "@ansyn/core": [
      "src/app/@ansyn/core/public_api"
    ],
    "@ansyn/core/*": [
      "src/app/@ansyn/core/*"
    ],
    "@ansyn/imagery": [
