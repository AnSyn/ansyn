    "@angular/router": "~10.0.5",
    "@ansyn/event-drops": "^0.2.8-1.9",
    "@ansyn/ng-pick-datetime": "^7.1.1",
    "@auth0/angular-jwt": "^4.0.0",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/entity": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "@ngx-translate/core": "^12.1.2",
    "@turf/turf": "^6.3.0",
    "angular2-uuid": "^1.1.1",
    "auth0-js": "^9.7.3",
    "auto-subscriptions": "^2.0.5",
