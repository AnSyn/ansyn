    "gl-matrix": "^2.8.1",
    "jspdf": "^2.1.1",
    "lodash": "^4.17.15",
    "lodash-decorators": "^4.5.0",
    "moment": "^2.21.0",
    "ngx-color-picker": "^9.1.0",
    "ngx-device-detector": "^1.4.1",
    "ol": "^6.4.3",
    "proj4": "^2.5.0",
    "rxjs": "~6.5.4",
    "shpjs": "^3.6.3",
    "tslib": "^2.0.0",
    "wellknown": "^0.5.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@angular/language-service": "~10.0.5",
    "@types/auth0-js": "^8.11.5",
    "@types/cesium": "^1.47.3",
    "@types/d3": "^4.13.1",
    "@types/file-saver": "^1.3.0",
    "@types/geojson": "7946.0.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "4.14.88",
    "@types/node": "^12.11.1",
    "@types/wellknown": "0.5.0",
    "codelyzer": "^6.0.0",
    "find-in-files": "0.4.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.5.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-jslint": "^1.0.10",
    "gulp-json-modify": "^1.0.2",
    "gulp-less": "^4.0.0",
    "gulp-ngc": "^0.3.3",
    "gulp-optimize-js": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sequence": "^1.0.0",
    "gulp-shell": "^0.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsdoc": "^3.6.3",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "pre-commit": "1.2.2",
    "protractor": "~7.0.0",
    "protractor-screenshoter-plugin": "^0.5.0",
    "replace": "^1.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "precommit": [
    "no-commit-to-master",
    "fix-all"
  ]
}
