		"jasmine-spec-reporter": "4.1.0",
		"jsdoc": "3.5.5",
		"karma": "1.7.0",
		"karma-chrome-launcher": "2.1.1",
		"karma-cli": "1.0.1",
		"karma-coverage-istanbul-reporter": "1.2.1",
		"karma-jasmine": "1.1.0",
		"karma-jasmine-html-reporter": "0.2.2",
		"lesshint": "4.4.0",
		"pre-commit": "1.2.2",
		"protractor": "5.1.2",
		"replace": "0.3.0",
		"ts-node": "3.2.0",
		"tslint": "5.7.0",
		"typescript": "2.4.2",
		"webpack": "3.10.0"
	}
