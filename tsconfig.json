{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@ansyn/*": [
        "src/app/@ansyn/*"
      ],
      "@ansyn/core": [
        "src/app/@ansyn/core/public_api"
      ],
      "@ansyn/imagery": [
        "src/app/@ansyn/imagery/public_api"
      ],
      "@ansyn/map-facade": [
        "src/app/@ansyn/map-facade/public_api"
      ],
      "@ansyn/event-drops": [
        "node_modules/@ansyn/event-drops"
      ],
      "@builder/*": [
        "src/app/builder/*"
      ],
      "configuration/*": [
        "src/configuration/*"
      ],
      "root/*": ["./*"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "es2017.object",
      "es2016.array.include",
      "dom"
    ]
  }
}
